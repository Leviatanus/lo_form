{
  "name": "website",
  "description": "LoForm's landing page",
  "version": "1.0.0",
  "private": "true",
  "license": "MIT",
  "scripts": {
    "watch:sass": "sass --watch src/_sass:app/web/assets/css",
    "build:sass": "sass src/_sass:app/web/assets/css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:eleventy": "npx @11ty/eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.3"
  }
}
